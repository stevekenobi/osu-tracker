{
  "name": "osu-tracker",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev:server": "nodemon src/index.ts NODE_OPTIONS='--max-old-space-size=512' --ignore tests/",
    "start": "node dist/src/index.js",
    "build": "npx tsc",
    "format": "prettier --write src/ tests/",
    "test:unit": "vitest tests/unit --test-timeout=60000 --hook-timeout=60000",
    "test:coverage": "vitest run tests/unit --coverage --test-timeout=60000 --hook-timeout=60000",
    "test:clients": "vitest run tests/clients --test-timeout=60000 --hook-timeout=60000",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --ignore-path .gitignore"
  },
  "author": "thanosbousios <bousiosthan@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/thanosbousios/osu-tracker.git",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@types/node-cron": "^3.0.11",
    "@types/numeral": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/coverage-v8": "^1.3.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "express": "^4.18.2",
    "google-auth-library": "^9.6.2",
    "google-spreadsheet": "^4.1.1",
    "node-cron": "^3.0.3",
    "numeral": "^2.0.6",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "prettier": "^3.0.3",
    "sequelize": "^6.31.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2",
    "vitest": "^1.3.1"
  }
}
